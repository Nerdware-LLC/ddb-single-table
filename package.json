{
  "name": "ddb-single-table",
  "version": "1.0.0",
  "description": "A DynamoDB modeling tool and client wrapper for NodeJS built for single-table designs.",
  "author": {
    "name": "Trevor Anderson",
    "email": "Trevor@Nerdware.cloud"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "type": "module",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.js"
    }
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Nerdware-LLC/ddb-single-table.git"
  },
  "homepage": "https://github.com/Nerdware-LLC/ddb-single-table#readme",
  "bugs": {
    "url": "https://github.com/Nerdware-LLC/ddb-single-table/issues"
  },
  "keywords": [
    "dynamodb",
    "ddb",
    "single-table"
  ],
  "scripts": {
    "lint": "eslint src",
    "build": "rm -rf ./dist && swc src --out-dir dist",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:ci": "vitest run --coverage --bail=1 --silent"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.398.0",
    "@aws-sdk/lib-dynamodb": "^3.398.0",
    "dayjs": "^1.11.9",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@serverless-guru/prettier-plugin-import-order": "^0.4.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.80",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitest/coverage-v8": "^0.34.3",
    "aws-sdk-client-mock": "^3.0.0",
    "eslint": "^8.48.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-vitest": "^0.2.8",
    "type-fest": "^4.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3",
    "vitest-github-actions-reporter": "^0.10.0"
  }
}
